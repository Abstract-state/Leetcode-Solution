TimeToClose_Hours = 
VAR timeText = TRIM(Sheet1[Time_To_Close])
RETURN 
IF(
    ISBLANK(timeText) || NOT CONTAINSSTRING(timeText, "h") || NOT CONTAINSSTRING(timeText, "m"),
    BLANK(),
    ROUND(
        VALUE(LEFT(timeText, FIND("h", timeText) - 1)) +
        VALUE(TRIM(SUBSTITUTE(MID(timeText, FIND("h", timeText) + 1, LEN(timeText)), "m", ""))) / 60,
        2
    )
)
